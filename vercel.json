{
    "version": 2,
    "builds": [
      {
        "src": "src/package.json",        
        "use": "@vercel/react"            
      },
      {
        "src": "server.js",               
        "use": "@vercel/node"             
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/public/$1"             
      },
      {
        "src": "/api/(.*)",
        "dest": "/server.js"             
      }
    ]
  }
  