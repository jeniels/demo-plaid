{
    "version": 2,
    "builds": [
      {
        "src": "src/index.js",            
        "use": "@vercel/static"           
      },
      {
        "src": "server.js",               
        "use": "@vercel/node"             
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/public/$1"             
      },
      {
        "src": "/api/(.*)",
        "dest": "/server.js"             
      }
    ]
  }
  